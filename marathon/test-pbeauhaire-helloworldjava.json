{
  "id": "test/pbeauhaire/hello-world-java",
  "cmd": "java -Xmx16m -Dhelloworld.port=$PORT0 -jar HelloWorld.jar",
  "cpus": 0.5,
  "mem": 128.0,
  "instances": 1,
  "ports": [0],
